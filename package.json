{
	"name": "frontend",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"lint:check": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives",
		"lint:fix": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --fix",
		"prettier:check": "prettier --ignore-path .gitignore --check 'src/**/*.{js,jsx,ts,tsx,css,html}'",
		"prettier:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,html}'",
		"lint-prettier-fix": "npm run lint:fix && npm run prettier:fix",
		"lint-prettier-check": "npm run lint:check && npm run prettier:check",
		"lint-staged": "npm run lint-prettier-fix && npm run lint-prettier-check"
	},
	"dependencies": {
		"axios": "^1.6.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.22.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.55",
		"@types/react-dom": "^18.2.19",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.35",
		"tailwindcss": "^3.4.1",
		"vite": "^5.1.0"
	}
}
